    "code-runner.executorMap": {
        "cpp": "cd $dirWithoutTrailingSlash && g++ -D __DEBUG -std=c++14 -Wall -Wextra -O2 -g -O2 -fsanitize=address -fsanitize=undefined -D_GLIBCXX_DEBUG $fileName -o bin/$fileNameWithoutExt && bin/$fileNameWithoutExt",
        "code-runner.clearPreviousOutput": false,
    },
    "explorer.autoReveal": false,
    "code-runner.ignoreSelection": true,
    "code-runner.temporaryFileName": "__tempCodeRunnerFile",
    "files.associations": {
        "array": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "sstream": "cpp",
        "regex": "cpp",
        "iostream": "cpp",
        "iosfwd": "cpp",
        "random": "cpp",
        "any": "cpp",
        "chrono": "cpp",
        "functional": "cpp",
        "istream": "cpp",
        "memory_resource": "cpp",
        "ostream": "cpp",
        "ratio": "cpp",
        "scoped_allocator": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "algorithm": "cpp",
        "limits": "cpp",
        "valarray": "cpp",
        "cmath": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "forward_list": "cpp",
        "initializer_list": "cpp",
        "numbers": "cpp"
    },
}